<?xml version="1.0" encoding="UTF-8" standalone="no"?>
<keys
   name="User Shortcuts">
  <bind
     gaction="app.org.inkstitch.auto-satin"
     keys="&lt;Primary&gt;equal" />
  <bind
     gaction="app.org.inkstitch.break-apart.noprefs"
     keys="&lt;Primary&gt;&lt;Shift&gt;o" />
  <bind
     gaction="app.org.inkstitch.commands"
     keys="&lt;Primary&gt;exclam" />
  <bind
     gaction="app.org.inkstitch.convert-to-satin.noprefs"
     keys="&lt;Primary&gt;&lt;Shift&gt;u" />
  <bind
     gaction="app.org.inkstitch.cut-satin.noprefs"
     keys="&lt;Primary&gt;&lt;Shift&gt;b" />
  <bind
     gaction="app.org.inkstitch.flip-satins.noprefs"
     keys="&lt;Primary&gt;&lt;Shift&gt;j" />
  <bind
     gaction="app.org.inkstitch.lettering"
     keys="&lt;Primary&gt;&lt;Shift&gt;q" />
  <bind
     gaction="app.org.inkstitch.params"
     keys="&lt;Primary&gt;&lt;Shift&gt;p" />
  <bind
     gaction="app.org.inkstitch.print.noprefs"
     keys="&lt;Primary&gt;&lt;Shift&gt;i" />
  <bind
     gaction="app.org.inkstitch.reorder.noprefs"
     keys="&lt;Primary&gt;apostrophe" />
  <bind
     gaction="app.org.inkstitch.simulator"
     keys="&lt;Primary&gt;&lt;Shift&gt;l" />
  <bind
     gaction="app.org.inkstitch.stitch-plan-preview.noprefs"
     keys="&lt;Primary&gt;greater" />
  <bind
     gaction="app.org.inkstitch.troubleshoot.noprefs"
     keys="&lt;Primary&gt;&lt;Shift&gt;BackSpace" />
  <bind
     gaction="app.selection-stack-down"
     keys="Page_Down" />
  <bind
     gaction="app.selection-stack-up"
     keys="Page_Up" />
  <bind
     gaction="win.path-reverse"
     keys="&lt;Primary&gt;r" />
</keys>
