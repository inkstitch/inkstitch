#: inx/inkstitch_knockdown_fill.inx:38
msgid "Round"
msgstr "Rund"

#: lib/elements/fill_stitch.py:189
msgid "Mitered"
msgstr "Spitz"

#: lib/elements/fill_stitch.py:189
msgid "Beveled"
msgstr "Abgeschrägt"

#: lib/elements/fill_stitch.py:195
msgid "Avoid self-crossing"
msgstr "Selbstüberschneidung vermeiden"

